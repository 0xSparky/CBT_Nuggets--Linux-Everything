#!/bin/bash

echo "This is a two number game.And you need to give two numbers in order to play."
echo "Enter First Number"
read num1

echo "Enter Second Number"
read num2

if [ $num1 = $num2 ]
then
	echo "They are the same. The number is $num1"
elif [ $num1 > $num2 ]
then
	echo "$num1 is greater than $num2"
elif [ $num1 < $num2 ]
then
	echo "$num1 is less then $num2"
else
	echo "You didn't input two numbers."
fi
